<div class="details">
  <dl class="field">
    <dt class="field-label">Advisor</dt>
    <dd class="value"><%= entity.advisor.name %></dd>
  </dl>
  <% if entity.agency %>
  <dl class="field">
    <dt class="field-label">Agency</dt>
    <dd class="value"><%= entity.agency.name %></dd>
  </dl>
  <% end %>
  <dl class="field">
    <dt class="field-label">State</dt>
    <dd class="value"><%= entity.state %></dd>
  </dl>
</div>